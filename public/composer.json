{
	"name": "silverstripe/installer",
	"description": "The SilverStripe Framework Installer",
	"repositories": [
		{
			"type": "vcs",
			"url": "https://github.com/titledk/silverstripe-gallery-pagetypes"
		}
	],
	"require": {
		"php": ">=5.3.3",
		"silverstripe/cms": "3.3.1",
		"silverstripe/framework": "3.3.1",
		"silverstripe/reports": "3.3.1",
		"silverstripe/siteconfig": "3.3.1",
		"titledk/silverstripe-gallery-pagetypes": "dev-master"
	},
	"require-dev": {
		"phpunit/PHPUnit": "~3.7",
		"froog/silvergraph": "dev-master",
		"axyr/silverstripe-ideannotator": "dev-master",
		"gdmedia/ss-auto-git-ignore": "^0.0.3",
		"oddnoc/silverstripe-artefactcleaner": "dev-master"
	},
	"extra": {
		"branch-alias": {
			"3.x-dev": "3.3.x-dev"
		}
	},
	"scripts": {
		"post-update-cmd": "GDM\\SSAutoGitIgnore\\UpdateScript::Go"
	},
	"prefer-stable": true,
	"minimum-stability": "dev"
}
